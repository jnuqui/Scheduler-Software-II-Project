      <Button layoutX="255.0" layoutY="414.0" mnemonicParsing="false" onAction="#updateCustomer" text="Update" />
      <Button layoutX="315.0" layoutY="414.0" mnemonicParsing="false" text="Reset" />
      <Button layoutX="357.0" layoutY="487.0" mnemonicParsing="false" onAction="#toCustomerGUI" text="Cancel" />
   </children>
</AnchorPane>
