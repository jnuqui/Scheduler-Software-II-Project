<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="534.0" prefWidth="452.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.UpdateCustomerController">
   <children>
      <Label layoutX="111.0" layoutY="147.0" text="Customer ID" />
      <Label layoutX="93.0" layoutY="187.0" text="Customer Name" />
      <Label layoutX="137.0" layoutY="225.0" text="Address" />
      <Label layoutX="118.0" layoutY="259.0" text="Postal Code" />
      <Label layoutX="142.0" layoutY="297.0" text="Phone" />
      <Label layoutX="80.0" layoutY="376.0" text="First Level Division" />
      <Label layoutX="132.0" layoutY="336.0" text="Country" />
      <TextField fx:id="customerIdTextfield" disable="true" layoutX="199.0" layoutY="143.0" prefHeight="25.0" prefWidth="68.0" promptText="(Auto)" />
      <TextField fx:id="customerNameTextField" layoutX="199.0" layoutY="183.0" />
      <TextField fx:id="addressTextField" layoutX="200.0" layoutY="221.0" prefHeight="25.0" prefWidth="177.0" />
      <TextField fx:id="postalCodeTextField" layoutX="199.0" layoutY="256.0" prefHeight="25.0" prefWidth="149.0" />
